{
  "name": "@taywa/swetch",
  "version": "3.2.2",
  "description": "Easily mock your real API data, simply by using your app.",
  "sideEffects": false,
  "scripts": {
    "start": "node src/scripts/server.mjs",
    "dev:server": "NODE_ENV=development nodemon -e mjs --exec \"clear && node dev/server.mjs\"",
    "dev:swetch": "NODE_ENV=development nodemon -e mjs --exec \"clear && node dev/swetch.mjs\"",
    "test": "vitest",
    "build": "rm -rf dist/* && rollup -c rollup.config.mjs",
    "prepublishOnly": "npm run build"
  },
  "author": "Taywa GmbH",
  "license": "GPL-3.0",
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rollup": "^3.21.0",
    "vite": "^4.3.1",
    "vitest": "^0.30.1"
  },
  "files": [
    "src/lib/*",
    "src/scripts/*",
    "dist/*"
  ],
  "keywords": [
    "mock",
    "api",
    "fetch"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bin": "./src/scripts/server.mjs"
}
